            <ColumnDefinition Width="151*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="_11" HorizontalAlignment="Left" Margin="2,81,0,0" VerticalAlignment="Top" Width="91" Height="65" Content="Q" Click="_11_Click" Grid.ColumnSpan="2" Grid.Column="2" FontSize="36"/>
