        <Button x:Name="_36" Content="C" HorizontalAlignment="Left" Margin="430,225,0,0" VerticalAlignment="Top" Width="91" Height="65" Grid.Column="3" Click="_36_Click" FontSize="36"/>
        <Button x:Name="_38" Content="B" HorizontalAlignment="Left" Margin="33,225,0,0" VerticalAlignment="Top" Width="91" Height="65" Grid.Column="4" Click="_38_Click" FontSize="36"/>
        <Button x:Name="_44" Content="2" HorizontalAlignment="Left" Margin="238,299,0,0" VerticalAlignment="Top" Width="91" Height="65" Grid.Column="3" Click="_44_Click" FontSize="36"/>
        <Button x:Name="_54" Content="0" HorizontalAlignment="Left" Margin="236,369,0,0" VerticalAlignment="Top" Width="91" Height="65" Grid.Column="3" Click="_54_Click" FontSize="36"/>
