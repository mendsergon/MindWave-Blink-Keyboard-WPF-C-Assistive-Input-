        <TextBox x:Name="LogOutput" Grid.Column="4" HorizontalAlignment="Left" Margin="33,-30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="91" Height="106" SelectionTextBrush="#FFF1FF00" Background="Black" Foreground="#FFF1FF00" IsEnabled="False" FontSize="10" TextChanged="LogOutput_TextChanged"/>
        <TextBox x:Name="TimerText" Grid.Column="2" HorizontalAlignment="Left" Margin="2,-30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="91" Height="106" SelectionTextBrush="#FFF1FF00" Background="Black" Foreground="#FFF1FF00" IsEnabled="False" FontSize="72" Grid.ColumnSpan="2" TextAlignment="Center" TextChanged="TimerText_TextChanged"/>

    </Grid>
</Window>
