        <Button x:Name="_28" Content="H" HorizontalAlignment="Left" Margin="33,152,0,0" VerticalAlignment="Top" Width="91" Height="65" Grid.Column="4" Click="_28_Click" FontSize="36"/>
        <Button x:Name="_33" Content="L" HorizontalAlignment="Left" Margin="140,225,0,0" VerticalAlignment="Top" Width="91" Height="65" Grid.Column="3" Click="_33_Click" FontSize="36"/>
