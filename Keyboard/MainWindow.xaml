        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Keyboard"
        mc:Ignorable="d"
        Title="mwKeyboard" Height="500" Width="800" Background="#FF656565">
    <Grid RenderTransformOrigin="0.094,0.08" UseLayoutRounding="False" Width="804" Margin="0,45,-4,-6" Background="#FF656565">
